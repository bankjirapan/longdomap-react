{
  "name": "react-longdo-map",
  "version": "1.0.0",
  "description": "Easy-to-use React component for Longdo Map.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "author": "longdo",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "keywords": [
    "react",
    "longdo",
    "longdo-map",
    "map",
    "react-component"
  ]
}
